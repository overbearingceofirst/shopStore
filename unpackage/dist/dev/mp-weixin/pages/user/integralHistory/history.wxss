page {
  position: relative;
  background-color: #F3F3F3;
}
.place {
  background-color: #ffffff;
  height: 100rpx;
}
.goods-list {
  width: 100%;
  padding: 0rpx 0 120rpx 0;
}
.goods-list .tis {
  width: 100%;
  height: 60rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 32rpx;
}
.goods-list .tis2 {
  width: 100%;
  height: 60rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  color: #777777;
  font-size: 26rpx;
}
.goods-list .row {
  width: calc(92%);
  padding: 0rpx 4%;
  height: 120rpx;
  background-color: #FFFFFF;
  margin: 20rpx auto;
  -webkit-box-shadow: 0rpx 5rpx 20rpx rgba(0, 0, 0, 0.1);
          box-shadow: 0rpx 5rpx 20rpx rgba(0, 0, 0, 0.1);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  z-index: 4;
  border: 0;
}
.goods-list .row > view {
  width: 50%;
  height: 100%;
  display: inline-block;
  float: left;
}
.goods-list .row .date {
  height: 100%;
  line-height: 120rpx;
  color: #777777;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.goods-list .row .status > view {
  height: 50%;
  line-height: 60rpx;
  text-align: right;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.goods-list .row .status .acquire {
  color: #EF5656;
}
.goods-list .row .status .noAcquire {
  color: #FF9327;
}

