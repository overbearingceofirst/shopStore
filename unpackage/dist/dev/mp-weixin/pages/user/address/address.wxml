<view><block wx:if="{{addressList.length>0}}"><view class="content"><view class="list"><block wx:for="{{addressList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['select',['$0'],[[['addressList','',index]]]]]]]}}" class="row" bindtap="__e"><view class="left"><view class="head">{{''+row.head+''}}</view></view><view class="center"><view class="name-tel"><view class="name">{{row.name}}</view><view class="tel">{{row.phoneNumber}}</view><block wx:if="{{row.defaultStatus===1}}"><view class="default">默认</view></block></view><view class="address">{{''+row.province+"-"+row.city+"-"+row.region+" "+row.detailAddress+''}}</view></view><view class="right"><view data-event-opts="{{[['tap',[['edit',['$0'],[[['addressList','',index]]]]]]]}}" class="icon bianji" catchtap="__e"></view></view></view></block></view></view></block><block wx:else><view class="noData">暂无收货地址</view></block><view class="add"><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="btn" bindtap="__e"><view class="icon tianjia"></view>新增地址</view></view></view>