}"><block><view data-event-opts="{{[['tap',[['remindDeliver',['$0'],['row']]]]]}}" class="default" bindtap="__e">提醒发货</view></block></block><block wx:if="{{item.$orig[0].status==2}}"><block><view data-event-opts="{{[['tap',[['showLogistics',['$0'],['row']]]]]}}" class="default" bindtap="__e">查看物流</view><view class="pay">确认收货</view><view class="pay">我要退货</view></block></block><block wx:if="{{item.$orig[0].status==3}}"><block><view class="default">再次购买</view></block></block><block wx:if="{{item.$orig[0].status=='4'}}"><block><view class="default">已取消</view></block></block></view></view></block></view></view></view>