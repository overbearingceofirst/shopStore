<view><view class="header"><view class="input-box"><view class="icon search"></view><input placeholder="请输入要搜索的关键字" placeholder-style="color:#c0c0c0;" data-event-opts="{{[['blur',[['e0',['$event']]]],['focus',[['e1',['$event']]]],['input',[['__set_model',['','inputData','$event',[]]]]]]}}" value="{{inputData}}" bindblur="__e" bindfocus="__e" bindinput="__e"/></view><view class="icon-btn"><view data-event-opts="{{[['tap',[['search']]]]}}" class="icon" style="font-size:30rpx;" bindtap="__e">搜索</view></view></view><view class="place"></view><block wx:if="{{showHistory}}"><view class="search_history"><view class="search_title"><view class="title_icon"><view class="search_title1">历史搜索</view><view data-event-opts="{{[['tap',[['clearHistory']]]]}}" class="icon shanchu" bindtap="__e"></view></view></view><block wx:if="{{historyList.length!==0}}"><view class="search_title _ul"><block wx:for="{{historyList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['inputAlain',['$0'],[[['historyList','',index]]]]]]]}}" bindtap="__e" class="_li">{{item}}</view></block></view></block><block wx:else><view class="noHistory">暂无搜索记录</view></block></view></block><block wx:if="{{showGoods}}"><view><view class="header2" style="{{'position:'+(headerPosition)+';'+('top:'+(headerTop)+';')}}"><block wx:for="{{orderbyList}}" wx:for-item="target" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['select',[index]]]]]}}" class="{{['target',target.selected?'on':'']}}" bindtap="__e">{{''+target.text+''}}<block wx:if="{{target.orderbyicon}}"><view class="{{['icon',target.orderbyicon[target.orderby]]}}"></view></block></view></block></view><view class="place"></view><view class="goods-list"><block wx:if="{{searchList.length>0}}"><view><view class="product-list"><block wx:for="{{searchList}}" wx:for-item="goods" wx:for-index="__i0__" wx:key="goods_id"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['searchList','goods_id',goods.goods_id]]]]]]]}}" class="product" bindtap="__e"><image mode="widthFix" src="{{goods.img}}"></image><view class="name">{{goods.name}}</view><view class="info"><view class="price">{{"积分"+goods.price}}</view></view></view></block></view><view class="loading-text">{{loadingText}}</view></view></block><block wx:else><view class="noProduct">暂无此类商品</view></block></view></view></block></view>