<view><block wx:if="{{showHeader}}"><view class="status" style="{{'position:'+(headerPosition)+';'+('top:'+(statusTop)+';')+('opacity:'+(afterHeaderOpacity)+';')}}"></view></block><block wx:if="{{showHeader}}"><view class="header" style="{{'position:'+(headerPosition)+';'+('top:'+(headerTop)+';')+('opacity:'+(afterHeaderOpacity)+';')}}"><view class="addr"><view class="icon location"></view>{{''+city+''}}</view><view class="input-box"><input placeholder="默认关键字" placeholder-style="color:#c0c0c0;" data-event-opts="{{[['tap',[['toSearch']]]]}}" bindtap="__e"/><view class="icon search"></view></view></view></block><block wx:if="{{showHeader}}"><view class="place"></view></block><view class="swiper"><view class="swiper-box"><swiper circular="true" autoplay="true" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{swiperList}}" wx:for-item="swiper" wx:for-index="__i0__" wx:key="id"><swiper-item><image src="{{swiper.img}}" data-event-opts="{{[['tap',[['toSwiper',['$0'],[[['swiperList','id',swiper.id]]]]]]]}}" bindtap="__e"></image></swiper-item></block></swiper><view class="indicator">--><block wx:for="{{swiperList}}" wx:for-item="swiper" wx:for-index="index" wx:key="index"><view class="{{['dots',currentSwiper>=index?'on':'']}}"></view></block></view></view></view><view class="category-list"><block wx:for="{{categoryList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toCategory',['$0'],[[['categoryList','',index]]]]]]]}}" class="category" bindtap="__e"><view class="img"><image src="{{row.img}}"></image></view><view class="text">{{row.name}}</view></view></block></view><view class="gifts"><view class="gifts_conent"><view class="gifts_title"><view class="text1">严选好礼</view><view class="text2">优质好礼严选</view></view><view class="gift_product"><block wx:for="{{niceGifts}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index<3}}"><view class="gift_products"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['niceGifts','',index]]]]]]]}}" class="product_conent" bindtap="__e"><view class="img"><image src="{{item.src}}" mode></image></view><view class="goods_name">{{item.name}}</view><view class="integral">{{"积分"+item.num}}</view></view></view></block></block></view></view></view><block wx:for="{{recommendList}}" wx:for-item="item" wx:for-index="__i1__" wx:key="url"><view class="recommend"><view class="img"><view class="recommend_img"><image src="{{item.url}}" mode></image></view></view><scroll-view class="scroll-view" scroll-x="true" show-scrollba="true"><block wx:for="{{item.chirldList}}" wx:for-item="item2" wx:for-index="__i2__" wx:key="src"><view class="recommend_good"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['recommendList','url',item.url],['chirldList','src',item2.src]]]]]]]}}" class="recommend_conent" bindtap="__e"><view class="img"><image src="{{item2.src}}" mode></image></view><view class="text1">{{item2.name}}</view><view class="text2">{{"积分"+item2.num}}</view></view></view></block></scroll-view></view></block><view class="goods-list"><view class="title">发现好物</view><view class="product-list"><block wx:for="{{productList}}" wx:for-item="product" wx:for-index="__i3__" wx:key="goods_id"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['productList','goods_id',product.goods_id]]]]]]]}}" class="product" bindtap="__e"><image mode="widthFix" src="{{product.img}}"></image><view class="name">{{product.name}}</view><view class="info"><view class="price">{{"积分"+product.price}}</view></view></view></block></view><view class="loading-text">{{loadingText}}</view></view></view>